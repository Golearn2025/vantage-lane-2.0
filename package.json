{
  "name": "vantage-lane-2.0",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_FONT_IGNORE_ERRORS=true next build",
    "build:clean": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:quiet": "eslint src --quiet",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "postinstall": "husky install",
    "audit:lint": "eslint src --max-warnings=0",
    "audit:types": "tsc --noEmit",
    "audit:test": "vitest run --reporter=verbose --run || echo 'No tests yet, continuing...'",
    "audit:build": "NEXT_FONT_IGNORE_ERRORS=true next build",
    "audit:custom": "node scripts/audit.js",
    "verify": "npm run audit:lint && npm run audit:types && npm run audit:custom",
    "quality-gate": "node scripts/quality-gate.js",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "clean": "rm -rf .next out node_modules/.cache",
    "prebuild": "npm run lint:quiet && npm run typecheck"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.6",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@sentry/nextjs": "^8.23.0",
    "@stripe/stripe-js": "^4.4.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.45.2",
    "@upstash/redis": "^1.28.4",
    "@vercel/analytics": "^1.3.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "envsafe": "^2.0.3",
    "framer-motion": "^11.18.2",
    "ky": "^1.7.2",
    "lucide-react": "^0.417.0",
    "nanoid": "^5.0.7",
    "next": "^15.0.2",
    "pino": "^9.0.0",
    "pino-pretty": "^10.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.52.2",
    "resend": "^4.0.0",
    "stripe": "^16.12.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.13",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.8.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.12.0",
    "eslint-config-next": "^15.5.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.4.0",
    "husky": "^9.0.11",
    "jsdom": "^27.0.0",
    "lint-staged": "^15.3.1",
    "playwright": "^1.48.2",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.33.5",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
